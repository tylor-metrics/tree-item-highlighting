{
  "name": "tree-item-highlighting",
  "displayName": "tree-item-highlighting",
  "description": "testing color styles when tree item focus changes",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
          {
              "id": "demo-tree-item-highlighting",
              "title": "TEST",
              "icon": "$(settings-gear)"
          }
      ]
    },
    "views": {
      "demo-tree-item-highlighting": [
        {
          "id": "tree-item-highlighting",
          "name": "tree item highlighting"
        }
      ]
    }
  },
  "scripts": {

		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint \"src/**/*.ts\""

  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9"
  }
}
